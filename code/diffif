void test(){

    int sockfd = socket(AF_INET, SOCK_STREAM, 0);
    struct sockaddr_in localaddr;
    localaddr.sin_family = AF_INET;
    localaddr.sin_port = 0; 

    localaddr.sin_addr.s_addr = inet_addr("192.168.1.1");
    
    if(bind(sockfd, (struct sockaddr *)&localaddr, sizeof(localaddr))==-1){
        
        localaddr.sin_addr.s_addr = INADDR_ANY;
        
        if(bind(sockfd, (struct sockaddr *)&localaddr, sizeof(localaddr))==-1){
            
            localaddr.sin_addr.s_addr = inet_addr(INADDR_ANY);
            
            if(bind(sockfd, (struct sockaddr *)&localaddr, sizeof(localaddr))==-1){
                localaddr.sin_addr.s_addr = htonl(INADDR_ANY);
                bind(sockfd, (struct sockaddr *)&localaddr, sizeof(localaddr));
            }

            localaddr.sin_addr.s_addr = "192.168.1.1";
            bind(sockfd, (struct sockaddr *)&localaddr, sizeof(localaddr));
        }
    }

    else if(bind(sockfd, (struct sockaddr *)&localaddr, sizeof(localaddr))==-1){

        char* userInput;
        gets(userInput);
        
    }

}