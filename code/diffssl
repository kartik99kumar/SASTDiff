RSA_public_decrypt(flen, from, to, key, RSA_PKCS1_PADDING); 
RSA_public_decrypt(flen, from, to, key, RSA_SSLV23_PADDING);
RSA_public_decrypt(flen, from, to, key, RSA_NO_PADDING);
SSL_connect(ssl1);
SSL_set1_host(ssl, HOST_NAME);
SSL_connect(ssl);

SSL_CTX_set_verify(ctx, SSL_VERIFY_NONE, NULL); 
SSL_CTX_set_verify(ctx, CURLOPT_SSL_VERIFYPEER, verify_callback);
SSL_CTX_set_verify(ctx, SSL_VERIFY_PEER, NULL);